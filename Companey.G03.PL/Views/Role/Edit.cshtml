
@model RoleViewModel
@{
    ViewData["Title"] = "Edit";
 
}

<h1>Edit</h1>

<form asp-action="Edit" enctype="multipart/form-data">
   
    <partial name="PartialView/EditCreateEmployeePartialView" model="Model"></partial>
    <a asp-action="AddOrRemoveUsers" asp-route-roleId="@Model.Id" class="btn btn-primary">AddOrRemoveUsre</a>
    
    <input type="submit" value="Save" class="btn btn-primary" asp-route-id="@Model.Id" />
    <a asp-action="Index" class="btn btn-light">Back To List</a>
</form>
@section Test {
    <script src="/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}